<launch>
    <!-- x y z yaw pitch roll -->

    <node pkg="tf" type="static_transform_publisher" name="tf_fake_drone"
        args="0 0 0.4 0 0 0 map drone 10"/>

    <node pkg="tf" type="static_transform_publisher" name="tf_d435_backward"
